<?xml version="1.0" encoding="utf-8"?>
<mesh:mesh xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:mesh="XMLMesh"
  xmlns:element="XMLMesh"
  xsi:schemaLocation="XMLMesh ../../xml_schemas/mesh_h2d_xml.xsd">
  
  <vertices>
  <!-- Contains all examples how it is possible to write a zero. -->
    <v x="0" y="0" i="0"/>
    <v x="4" y="0" i="1"/>
    <v x="4" y="1" i="2"/>
    <v x="0" y="1" i="3"/>
    <v x="0.166666666666666666" y="0" i="4"/>
    <v x="0.166666666666666666" y="1" i="5"/>
  </vertices>

  <elements>
    <mesh:q v1="0" v2="4" v3="5" v4="3" m="Post" />
    <mesh:q v1="4" v2="1" v3="2" v4="5" m="Pre" />
  </elements>

  <edges>
    <ed v1="0" v2="4" m="BottomPost" />
    <ed v1="4" v2="1" m="BottomRefl" />
    <ed v1="1" v2="2" m="Right" />
    <ed v1="2" v2="5" m="TopPre" />
    <ed v1="5" v2="3" m="TopPost" />
    <ed v1="3" v2="0" m="Left" />
  </edges>
</mesh:mesh>